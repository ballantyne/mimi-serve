{
  "name": "Mimi Serve",
  "description": "A proxy to receive mimis",
  "repository": "https://github.com/ballantyne/mimi-server",
  "logo": "https://github.com/ballantyne/mimi-serve/raw/master/mimi.png",
  "keywords": ["mimi"],
  "env": {
    "AWS_ACCESS_KEY_ID": {
      "description": "Set your AWS Access Key ID to enable S3 file storage.",
      "required": true
    },
    "AWS_ACCESS_SECRET_KEY": {
      "description": "AWS Access Secret Key.",
      "required": true
    },
    "AWS_BUCKET": {
      "description": "Name of your S3 bucket on AWS.",
      "required": true
    },
    "AWS_REGION": {
      "description": "Region of your S3 bucket on AWS.",
      "required": true
    }
  }
}
